{
	"name": "clover",
	"displayName": "Clover",
	"description": "Socket REPL development for your editor",
	"publisher": "mauricioszabo",
	"repository": "https://github.com/mauricioszabo/clover/",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.connectSocketRepl"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.connectSocketRepl",
				"title": "Connect Socket REPL"
			},
			{
				"command": "clover.evaluate-block",
				"title": "Evaluate current block of code"
			},
			{
				"command": "clover.evaluate-top-block",
				"title": "Evaluate current top-block of code"
			},
			{
				"command": "clover.evaluate-selection",
				"title": "Evaluate current selection of code"
			},
			{
				"command": "clover.clear-console",
				"title": "Clear Clojure console"
			},
			{
				"command": "clover.disconnect",
				"title": "Disconnect all REPLs"
			},
			{
				"command": "clover.load-file",
				"title": "Load File in current REPL"
			},
			{
				"command": "clover.break-evaluation",
				"title": "Break Clojure Evaluation"
			},
			{
				"command": "clover.doc-for-var",
				"title": "Show Documentation for Var"
			},
			{
				"command": "clover.connect-embedded",
				"title": "Start Shadow-CLJS' ClojureScript REPL"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"shadow-cljs": "^2.8.83",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"ansi_up": "^4.0.4",
		"create-react-class": "^15.6.3",
		"react": "^16.9.0",
		"react-dom": "^16.9.0"
	},
	"__metadata": {
		"id": "46228898-6f2d-4345-b0a1-110cecdc9866",
		"publisherDisplayName": "Maur√≠cio Szabo",
		"publisherId": "5954deb7-f27b-4b00-8b5b-22970a2bc859"
	}
}
