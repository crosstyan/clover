{
	"name": "clover",
	"displayName": "Clover",
	"description": "Socket REPL development for your editor",
        "publisher": "mauricioszabo",
        "repository":"https://github.com/mauricioszabo/clover/",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.connectSocketRepl",
		"onCommand:extension.connectCljsSocketRepl"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.connectSocketRepl",
				"title": "Connect Clojure Socket REPL"
			},
			{
				"command": "clover.evaluate-block",
				"title": "Evaluate current block of code"
			},
			{
				"command": "clover.evaluate-top-block",
				"title": "Evaluate current top-block of code"
			},
			{
				"command": "clover.evaluate-selection",
				"title": "Evaluate current selection of code"
			},
			{
				"command": "clover.clear-console",
				"title": "Clear Clojure console"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"eslint": "^5.13.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"shadow-cljs": "^2.8.52",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"ansi_up": "^4.0.4",
		"create-react-class": "^15.6.3",
		"react": "^16.9.0",
		"react-dom": "^16.9.0"
	}
}
